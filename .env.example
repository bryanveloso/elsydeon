# Discord configuration
DISCORD_TOKEN=your_discord_token_here

# Twitch configuration
TWITCH_CLIENT_ID=your_twitch_client_id_here
TWITCH_CLIENT_SECRET=your_twitch_client_secret_here
TWITCH_CHANNELS=channel1,channel2
TWITCH_USER_ID=your_twitch_user_id_here

# Database configuration
DATABASE_PATH=quotes.db

# LM Studio configuration
# Use localhost for development, saya.local for production
LM_STUDIO_API_URL=http://localhost:1234/v1
LM_STUDIO_API_KEY=
LM_STUDIO_MODEL=llama-3.2-3b-instruct

# Synthform API configuration
# Point to your Synthform instance on the Tailscale network
SYNTHFORM_API_URL=http://localhost:8000/api

# Redis configuration
# Point to the Redis instance where Synthform is running
REDIS_URL=redis://localhost:6379
